<form method="post" action="{{ path('utilisateur_delete', {'id': utilisateur.id}) }}" onsubmit="return confirm('Are you sure you want to delete this user?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ utilisateur.id) }}">
    <input type="hidden" name="_url" value="{{ _url }}">
    <button class="btn"><i class="bi bi-trash text-danger"></i></button>
</form>
